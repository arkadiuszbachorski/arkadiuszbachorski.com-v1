{
    "extends": ["airbnb", "prettier", "prettier/react"],
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/require-default-props": 0,
        "react/jsx-props-no-spreading": 0
    },
    "env": {
        "jest": true,
        "serviceworker": true
    },
    "globals": {
        "document": true,
        "window": true
    },
    "settings": {
        "import/resolver": {
            "node": {
                "paths": ["src"],
                "extensions": [".js", ".jsx", ".ts", ".tsx"]
            }
        }
    }
}
