    },
    "keywords": [
        "arkadiusz",
        "bachorski",
        "programmer"
    ],
    "license": "MIT",
    "scripts": {
        "build": "gatsby build",
        "develop": "gatsby develop",
        "format": "prettier --write \"src/**/*.{js,css,json}\"",
        "start": "npm run develop",
        "serve": "gatsby serve",
        "clean": "gatsby clean",
        "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/arkadiuszbachorski/arkadiuszbachorski.com"
    },
    "prettier": {
        "singleQuote": true,
        "trailingComma": "all",
        "printWidth": 120,
        "tabWidth": 4,
        "semi": true,
        "quoteProps": "preserve"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "src/**/*.(js|css|json)": [
            "prettier --write"
        ]
    }
